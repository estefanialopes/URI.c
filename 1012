#include <stdio.h>
 
int main() {
 
    double A,B,C,X,Triangulo,Circulo,trapezio,quad,retangulo;
     scanf("%lf %lf %lf",&A,&B,&C);
    
    double pi = 3.14159;
    Triangulo = A*C/2;
    Circulo = pi*C*C;
    X=A+B;
    trapezio = X*C/2;
    quad = B*B;
    retangulo = A*B;
    printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n",Triangulo,Circulo,trapezio,quad,retangulo);
 
    return 0;
}
